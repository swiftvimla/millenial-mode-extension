{
	"name": "millennial-mode-extension",
	"displayName": "millennial-mode-extension",
  "description": "As a millennial, I need immediate feedback or I'll give up in frustration.",
  "repository": { "url": "https://github.com/swiftvimla/millennial-mode-extension" },
  "version": "1.0.0",
  "publisher": "southpaw-industries",
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:*.js"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "millennial-mode-extension.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "millennial-mode-extension.millennialMode",
				"title": "Open Millennial Mode editor"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.43.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"@typescript-eslint/parser": "^2.26.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vsce": "^1.75.0",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"preact": "^10.4.0"
	}
}
